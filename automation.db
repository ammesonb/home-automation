--
-- Name: create_ts_range(); Type: FUNCTION; Schema: public; Owner: brett
--

CREATE FUNCTION create_ts_range() RETURNS tsrange
    LANGUAGE plpgsql
    AS $$
BEGIN
return tsrange((select '2000-01-01 ' || to_char((select now()), 'HH:MI'))::timestamp, (select '2000-01-01 ' || to_char((select (now() + interval '1 minute')), 'HH:MI'))::timestamp);
END;
$$;


ALTER FUNCTION public.create_ts_range() OWNER TO brett;

